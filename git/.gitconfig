[user]
	name = username
	email = email@gmail.com
[core]
	editor = vim
[credential]
	helper = osxkeychain
[push]
	default = simple
[init]
        templateDir = ~/.git-template/
[color]
    	branch = auto
    	diff = auto
    	status = auto
[color "branch"]
   	 current = yellow reverse
    	local = yellow
    	remote = green
[color "diff"]
    	meta = yellow bold
    	frag = magenta bold
    	old = red bold
    	new = green bold
[color "status"]
    	added = yellow
    	changed = green
    	untracked = cyan
