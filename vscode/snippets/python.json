	"env": {
		"prefix": ["env", "#!"] ,
		"body": [
			"#!/usr/bin/env python"
		],
		"description": "python enviroment"
	},

	"importboiler": {
		"prefix": ["import"] ,
		"body": [
			"import os",
			"import sys",
			"import numpy as np",
			"import pandas as pd"
		],
		"description": "boilerplate imports"
	},

	"importplot": {
		"prefix": ["import", "matplotlib", "plt"] ,
		"body": [
			"import matplotlib.pyplot as plt"
		],
		"description": "import matplotlib"
	},

	"importlplot": {
		"prefix": ["import", "lplot"] ,
		"body": [
			"from mypylib import lplot"
		],
		"description": "import custom plotting lib"
	},

	"importcmd": {
		"prefix": ["cmd", "import"] ,
		"body": [
			"from mypylib.lutil import cmd"
		],
		"description": "import shell command function"
	},


	"defmain": {
		"prefix": ["main", "__name"] ,
		"body": [
			"def main():",
			"    print(\"hello world\")",
			"",
			"",
			"if __name__ == \"__main__\":",
			"    main()"
		],
		"description": "boilerplate main function"
	}

}
